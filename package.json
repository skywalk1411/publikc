{
  "name": "publikc",
  "version": "1.2.0",
  "description": "Electron client for Kirka.io",
  "main": "dist/main.js",
  "scripts": {
    "start": "npm run compile && electron .",
    "compile": "tsc && npm run copy-assets",
    "compile:prod": "tsc && npm run copy-assets && node scripts/minify.js",
    "copy-assets": "copyfiles -u 1 \"src/assets/**/*\" dist/ && copyfiles -u 1 \"src/util/defaults.json\" dist/",
    "build": "npm run compile:prod && electron-builder",
    "dev": "npm run compile && electron ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skywalk1411/publikc.git"
  },
  "keywords": [],
  "author": "skywalk, irrvlo",
  "license": "ISC",
  "devDependencies": {
    "@types/electron": "^1.4.38",
    "@types/node": "^24.6.2",
    "copyfiles": "^2.4.1",
    "electron": "^10.4.7",
    "electron-builder": "^23.0.2",
    "terser": "^5.44.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "discord-rpc": "^4.0.1",
    "electron-localshortcut": "^3.2.1",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.2.1",
    "v8-compile-cache": "^2.4.0"
  },
  "optionalDependencies": {
    "@juice-client/node-enject": "^1.0.3",
    "dmg-license": "^1.0.11"
  }
}
